{
  "name": "embeddable.com-boilerplate-repo",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "ct": "tsconfig --noEmit",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "embeddable:build": "embeddable build",
    "embeddable:dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' embeddable dev",
    "embeddable:login": "embeddable login",
    "embeddable:push": "embeddable push",
    "embeddable:upgrade": "npx npm-check-updates -u \"/embeddable/\"",
    "reinstall": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@embeddable.com/remarkable-pro": "^0.1.7"
  },
  "devDependencies": {
    "@embeddable.com/core": "^2.13.6",
    "@embeddable.com/react": "^2.13.4",
    "@embeddable.com/sdk-core": "^4.1.12",
    "@embeddable.com/sdk-react": "^4.2.4",
    "@swc/core": "1.13.5",
    "@types/react": "^19.1.13",
    "cross-env": "^7.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "overrides": {
    "glob": "^13.0.0"
  }
}
